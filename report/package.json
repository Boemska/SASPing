{
  "name": "sasping_report",
  "version": "0.1.0",
  "description": "",
  "main": "index.js",
  "watch": {
    "style": "style.scss",
    "copy:index.html": "app/index.html"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "parallelshell 'http-server . -a localhost -p 8000 -c-1' 'npm-watch'",
    "style": "mkdir -p build && ./node_modules/node-sass/bin/node-sass app/style.scss > build/style.css",
    "mkbuild": "rm -r build; mkdir -p build build/js;",
    "build:min": "npm run style && ./node_modules/usemin-cli/bin/usemin app/index.html -d dist -o dist/index.html",
    "pack:prod:index": "./node_modules/webpack/bin/webpack.js -p --output-public-path js/ app/js/index.js dist/js/index.js",
    "pack:prod:vendors": "./node_modules/webpack/bin/webpack.js -p --output-public-path js/ app/js/vendors.js dist/js/vendors.js",
    "pack:prod": "parallelshell 'npm run pack:prod:index' 'npm run pack:prod:vendors'",
    "release": "mkdir -p dist/js dist/css build; parallelshell 'npm run build:min' 'npm run pack:prod'",
    "pack:dev:vendors": "./node_modules/webpack/bin/webpack.js -d --output-public-path js/ --watch app/js/vendors.js build/js/vendors.js",
    "pack:dev:index": "./node_modules/webpack/bin/webpack.js -d --output-public-path js/ --watch app/js/index.js build/js/index.js",
    "pack:dev": "parallelshell 'npm run pack:dev:index' 'npm run pack:dev:vendors'",
    "copy:index.html": "./node_modules/copyfiles/copyfiles -f app/index.html build",
    "copy:csv": "./node_modules/copyfiles/copyfiles -f app/sasping_data.csv build",
    "copy": "parallelshell 'npm run copy:index.html' 'npm run copy:csv'",
    "dev": "npm run mkbuild; parallelshell 'npm run pack:dev' 'npm run copy' 'npm start'"
  },
  "author": "bojan@boemskats.com",
  "license": "ISC",
  "dependencies": {
    "blaze": "^3.2.0",
    "compute-iqr": "^1.1.0",
    "d3": "^3.5.17",
    "nvd3": "^1.8.5",
    "papaparse": "^4.1.2"
  },
  "devDependencies": {
    "copyfiles": "^1.0.0",
    "node-sass": "^4.3.0",
    "npm-watch": "^0.1.7",
    "parallelshell": "^2.0.0",
    "replace": "^0.3.0",
    "usemin-cli": "^0.2.0",
    "webpack": "^1.14.0",
    "worker-loader": "^0.7.1"
  }
}
